#include <iostream>
using namespace std;

void swap(int a, int b) {
    int temp = a;
    a = b;
    b = temp;
    cout << "Inside function after swapping: a = " << a << ", b = " << b << endl;
}

int main() {
    int x, y;
    cout << "Enter two numbers:" << endl;
    cin >> x >> y;

    cout << "Before swapping (main): x = " << x << ", y = " << y << endl;
    swap(x, y);
    cout << "After swapping (main): x = " << x << ", y = " << y << endl;

    return 0;
}
